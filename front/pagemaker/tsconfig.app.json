{
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "target": "es2017",
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "strict": true,
    "module": "commonjs",
        "composite": true,
        "baseUrl": ".",
        "paths": {
          "@/*": ["./src/*"]
        }
  }
}
